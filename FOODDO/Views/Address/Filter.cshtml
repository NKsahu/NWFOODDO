@model IEnumerable<FOODDO.Models.Address>
@{ Layout = null;}
<table class="table table-bordered table-hover" id="TBLID">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Landmark)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>

        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Landmark)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Hub)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>

            </tr>
        }
    </tbody>
</table>
